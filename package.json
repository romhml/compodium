{
  "name": "compodium",
  "version": "1.0.0",
  "description": "My new Nuxt module",
  "repository": "romhml/compodium",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "COMPODIUM_LOCAL=true nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "typecheck": "vue-tsc --noEmit && nuxi typecheck devtools && nuxi typecheck playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.15.4",
    "@vueuse/core": "^12.5.0",
    "nuxt-component-meta": "^0.10.0",
    "sirv": "^3.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.0.0",
    "@nuxt/eslint-config": "^1.0.1",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.4",
    "@nuxt/test-utils": "^3.15.4",
    "@types/node": "latest",
    "changelogen": "^0.5.7",
    "eslint": "^9.19.0",
    "happy-dom": "^17.0.0",
    "nuxt": "^3.15.4",
    "playwright-core": "^1.50.1",
    "typescript": "~5.7.3",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.2.0"
  },
  "packageManager": "pnpm@8.15.5+sha512.b051a32c7e695833b84926d3b29b8cca57254b589f0649d899c6e9d0edb670b91ec7e2a43459bae73759bb5ce619c3266f116bf931ce22d1ef1759a7e45aa96f"
}
