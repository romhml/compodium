{
  "name": "compodium",
  "type": "module",
  "version": "0.1.0",
  "description": "A plug and play component playground for Vue and Nuxt.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/unplugin-starter.git"
  },
  "keywords": [
    "nuxt",
    "components",
    "documentation"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:prepare": "tsup",
    "build": "tsup",
    "dev": "tsup --watch src",
    "test": "vitest"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "vite": ">=3"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  },
  "dependencies": {
    "unplugin": "^2.2.0",
    "@compodium/meta": "workspace:*",
    "micromatch": "^4.0.8",
    "npm-run-path": "^6.0.0",
    "pathe": "^2.0.3",
    "scule": "^1.3.0",
    "semver": "^7.7.1",
    "sirv": "^3.0.1",
    "tinyglobby": "^0.2.12",
    "ufo": "^1.5.4",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.15.4",
    "@nuxt/schema": "^3.15.4",
    "@types/node": "^22.13.1",
    "eslint": "^9.20.0",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  }
}
