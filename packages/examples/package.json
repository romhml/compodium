{
  "name": "@compodium/examples",
  "version": "1.0.0",
  "description": "Third party library examples for Compodium",
  "type": "module",
  "repository": {
    "url": "romhml/compodium",
    "directory": "packages/examples"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "dev:prepare": "unbuild && cp -r src/examples dist/examples",
    "prepack": "unbuild && cp -r src/examples dist/examples"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.5.2",
  "devDependencies": {
    "unbuild": "^3.5.0"
  },
  "dependencies": {
    "scule": "^1.3.0"
  }
}
