{
  "name": "@compodium/examples",
  "version": "0.1.0-beta.7",
  "description": "Third party library examples for Compodium",
  "type": "module",
  "license": "MIT",
  "repository": {
    "url": "romhml/compodium",
    "directory": "packages/examples"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./*": "./dist/*"
  },
  "lib": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:prepare": "tailwindcss -i src/assets/ui/base.css -o src/assets/ui/index.css && unbuild",
    "dev": "unbuild --watch",
    "prepack": "unbuild"
  },
  "dependencies": {
    "pathe": "^2.0.3",
    "scule": "^1.3.0"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.0.14",
    "tailwindcss": "^4.0.14"
  }
}
