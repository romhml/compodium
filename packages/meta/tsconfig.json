{
  "compilerOptions": {
    "outDir": "./dist",
    "declaration": true,
    "emitDeclarationOnly": false,
    "declarationMap": true,
    "target": "esnext",
    "module": "NodeNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "nodenext",
    "paths": {
      "#module/*": ["../src/*"]
    }
  },
  "include": ["lib/**/*"]
}
