{
  "name": "@compodium/nuxt",
  "version": "0.1.0-beta.12",
  "description": "A plug an play component playground for Nuxt.",
  "repository": {
    "url": "romhml/compodium",
    "directory": "packages/nuxt"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "release": "pnpm run lint && pnpm run test && pnpm run prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "typecheck": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "@nuxt/kit": ">=3.15",
    "@nuxt/test-utils": ">=3.20",
    "vite": ">=6"
  },
  "peerDependenciesMeta": {
    "@nuxt/test-utils": {
      "optional": true
    }
  },
  "dependencies": {
    "@compodium/core": "workspace:^",
    "@nuxt/devtools-kit": "^2.6.5",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "ufo": "^1.6.1"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "catalog:",
    "nuxt": "catalog: ",
    "pathe": "^2.0.3"
  },
  "keywords": [
    "nuxt",
    "components",
    "documentation"
  ]
}
