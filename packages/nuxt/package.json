{
  "name": "@compodium/nuxt",
  "version": "0.1.0-beta.5",
  "description": "A plug an play component playground for Nuxt.",
  "repository": {
    "url": "romhml/compodium",
    "directory": "packages/nuxt"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build && nuxi generate devtools",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "release": "pnpm run lint && pnpm run test && pnpm run prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run ."
  },
  "dependencies": {
    "@compodium/core": "workspace:*",
    "@nuxt/devtools-kit": "^2.2.1",
    "@nuxt/kit": "^3.15.4",
    "consola": "^3.4.0",
    "ufo": "^1.5.4"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.4",
    "nuxt": "^3.15.4",
    "pathe": "^2.0.3"
  },
  "keywords": [
    "nuxt",
    "components",
    "documentation"
  ]
}
