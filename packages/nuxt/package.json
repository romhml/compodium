{
  "name": "@compodium/nuxt",
  "version": "0.1.0-beta.5",
  "description": "A plug an play component playground for Nuxt.",
  "repository": {
    "url": "romhml/compodium",
    "directory": "packages/nuxt"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build  && cp ../LICENSE.md ../README.md ./dist",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "release": "pnpm run lint && pnpm run test && pnpm run prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run ."
  },
  "dependencies": {
    "@compodium/core": "workspace:*",
    "@nuxt/devtools-kit": "^2.2.1",
    "@nuxt/kit": "^3.16.0",
    "@vueuse/core": "^13.0.0",
    "chokidar": "^4.0.3",
    "consola": "^3.4.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.2.1",
    "@nuxt/eslint-config": "^1.2.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.16.0",
    "@nuxt/test-utils": "^3.17.2",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^22.13.10",
    "@types/semver": "^7.5.8",
    "happy-dom": "^16.8.1",
    "nuxt": "^3.16.0",
    "playwright-core": "^1.51.0",
    "vue-tsc": "^2.2.0"
  },
  "keywords": [
    "nuxt",
    "components",
    "documentation"
  ]
}
