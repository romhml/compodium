{
  "name": "@compodium/vue",
  "type": "module",
  "version": "0.1.0-beta.12",
  "description": "A plug and play component playground for Vue and Nuxt.",
  "license": "MIT",
  "repository": {
    "url": "romhml/compodium",
    "directory": "packages/vue"
  },
  "keywords": [
    "vue",
    "components",
    "documentation"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    },
    "./*": "./dist/*"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:prepare": "unbuild --stub",
    "prepack": "unbuild",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vite": ">=6"
  },
  "dependencies": {
    "@compodium/core": "workspace:^",
    "@compodium/examples": "workspace:^",
    "@vue/devtools-kit": "^8.0.3",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "mlly": "^1.8.0",
    "pathe": "^2.0.3",
    "ufo": "^1.6.1",
    "unplugin": "^2.3.10"
  }
}
